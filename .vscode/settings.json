{
    "workbench.colorCustomizations": {
        "editorLineNumber.foreground": "#c3c3c3",
        "editorLineNumber.activeForeground": "#fff",
        "editor.selectionHighlightBorder": "#edededaa",
        "editor.wordHighlightBorder": "#ededed",
        "activityBar.foreground": "#fff",
        "activityBar.background": "#133326",
        "titleBar.activeBackground": "#1B4835",
        "titleBar.activeForeground": "#F6FBF9"
    },
    "prettier.configPath": ".prettierrc",
    "prettier.requireConfig": true,
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
}